{
	"name": "the-ink-press",
	"version": "0.1.0",
	"private": true,
	"description": "Daily news -> summary -> image prompt -> generated image (CLI).",
	"bin": {
		"the-ink-press": "./dist/index.js"
	},
	"scripts": {
		"dev": "bun run src/index.ts",
		"build": "bun build src/index.ts --target=bun --outdir=dist --minify",
		"compile": "bun build src/index.ts --compile --minify --outfile=dist/the-ink-press",
		"start": "bun dist/index.js",
		"typecheck": "tsc --noEmit",
		"lint": "biome check",
		"format": "biome check --write"
	},
	"dependencies": {
		"@ai-sdk/openai": "^3.0.23",
		"@napi-rs/image": "^1.12.0",
		"ai": "^6.0.64",
		"commander": "^14.0.3",
		"fast-xml-parser": "^5.3.4",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@types/bun": "latest",
		"typescript": "^5.9.3"
	},
	"engines": {
		"bun": ">=1.0.0"
	}
}
